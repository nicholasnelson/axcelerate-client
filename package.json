{
	"name": "axcelerate-client",
	"version": "0.1.0",
	"description": "A TypeScript client for the aXcelerate API. Provides a simple, typed wrapper around aXcelerate's REST endpoints (contacts, courses, enrolments, and more) for Node.js server-side applications.",
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup src/index.ts --dts --format esm,cjs",
		"dev": "tsup src/index.ts --watch",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"lint": "eslint .",
		"format": "prettier . --write",
		"clean": "rimraf dist",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nicholasnelson/axcelerate-client.git"
	},
	"keywords": [
		"axcelerate",
		"api",
		"client",
		"typescript",
		"nodejs",
		"wrapper",
		"sdk"
	],
	"author": {
		"name": "Nicholas Nelson",
		"url": "https://github.com/nicholasnelson"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/nicholasnelson/axcelerate-client/issues"
	},
	"homepage": "https://github.com/nicholasnelson/axcelerate-client#readme",
	"devDependencies": {
		"@eslint/js": "^9.35.0",
		"@types/node": "^24.3.1",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"jiti": "^2.5.1",
		"lint-staged": "^16.1.6",
		"prettier": "^3.6.2",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.43.0",
		"undici": "^7.16.0",
		"vitest": "^3.2.4"
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix",
		"*.--write": "prettier --write"
	},
	"dependencies": {
		"zod": "^4.1.5"
	}
}
